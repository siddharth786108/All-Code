class Solution {
public:
    vector<int> getConcatenation(vector<int>& nums) {
        vector<int> sum;
        int n=nums.size();
        for(int i=0;i<n;i++)
        {
            sum.push_back(nums[i]);
        }
        for(int i=0;i<sum.size();i++)
        {
            nums.push_back(sum[i]);
        }
        
    return nums;    
    }
};
