#include<iostream>
using namespace std;
int main()
{
    int n=5;
    int arr[n]={1,5,3,2,4};
    int start=0,end=n-1,temp=0;
    while(start<=end)
    {
        temp=arr[start];
        arr[start]=arr[end];
        arr[end]=temp;
        start++;
        end--;
    }
    for(int i=0;i<n;i++)
    {
        cout<<arr[i];
    }
    
    
}
