#include<iostream>
#include<vector>
using namespace std;
int main()
{
    vector<int> a={5,4,3,2,1};
    for(int i=0;i<a.size();i++)
    {
        cout<<a[i];
    }
    cout<<endl<<endl;
    vector<int> b=a;        // copy a in b
    for(int i=0;i<b.size();i++)
    {
        cout<<b[i];
    }
    return 0;
}
