#include<iostream>
#include<vector>
#include<algorithm>
using namespace std;
int main()
{
    vector<int> array;
    array.push_back(10);
    array.push_back(20);
    array.push_back(30);
    array.push_back(40);
    array.push_back(50);
    array.push_back(60);
    for(int i=0;i<array.size();i++)
    {
        cout<<array[i]<<" ";
    }
    cout<<endl<<endl;
    array.erase(array.begin()+2,array.begin()+5);
    /*
    Or we can use this   
    array.erase(array.begin()+2,array.end()-1); 
    */
    for(int i=0;i<array.size();i++)
    {
        cout<<array[i]<<" ";
    }
}
