#include<iostream>
#include<queue>
using namespace std;
int main()
{
    queue<int> q;
    q.push(10);
    q.push(20);
    q.push(30);
    q.push(40);
    cout<<"Size of queue before pop "<<q.size()<<endl;
    q.pop();
    cout<<"Size of queue after pop "<<q.size()<<endl;
    cout<<"Front Element "<<q.front()<<endl;
    cout<<"Queue is Empty or Not "<<q.empty();
}
