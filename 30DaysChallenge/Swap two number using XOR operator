#include <iostream>
using namespace std;
int main(void)
{
    int a,b;
    cout<<"Enter two value to sawap:"<<endl;
    cin>>a>>b;
    cout<<a<<" "<<b<<endl;
    a=a+b;
    b=a-b;
    a=a-b;
    cout<<"After swap value will be:"<<endl;
    cout<<a<<" "<<b<<endl<<endl;
    
    // Using XOR method:
    
    int c,d;
    cout<<"Enter two value to sawap:"<<endl;
    cin>>c>>d;
    cout<<c<<" "<<d<<endl;
    c=c^d;
    d=c^d;
    c=c^d;
    cout<<"After swap value will be:"<<endl;
    cout<<c<<" "<<d;
    return 0;
}
